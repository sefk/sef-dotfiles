#!/usr/bin/env python

import sys
import random

def unsort():
    if len(sys.argv) == 1:
        f = sys.stdin
    elif len(sys.argv) == 2:
        f = open(sys.argv[1])
    else:
        sys.stderr.write("usage: %s [infile]")
        sys.exit(1)

    content = f.readlines()
    random.shuffle(content)
    for i in content:
        sys.stdout.write(i)

if __name__ == "__main__":
    unsort()
