# Homebrew stuff
# This is for running homebrew in my home directory. No the standard setup but works with
# Google configurations
export PATH=$HOME/homebrew/bin:$PATH
export LD_LIBRARY_PATH=$HOME/homebrew/lib:$LD_LIBRARY_PATH

